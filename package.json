{
  "name": "@folio-eis/marva-next",
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/marva-next.es.js",
      "require": "./dist/marva-next.umd.js"
    }
  },
  "main": "./dist/marva-next.umd.js",
  "module": "./dist/marva-next.es.js",
  "scripts": {
    "dev": "vite",
    "build": "export EDITOR_BUILD_TYPE=0 && tsc && vite build",
    "build:lib": "export EDITOR_BUILD_TYPE=1 && tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier:format:all": "prettier --write ./src/**/*.{ts,tsx,json,scss}"
  },
  "dependencies": {
    "@r2wc/react-to-web-component": "^2.0.2",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^5.7.3",
    "recoil": "^0.7.7"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.0",
    "vite-plugin-css-injected-by-js": "^3.1.1"
  }
}
